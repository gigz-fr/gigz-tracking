{
  "name": "gigz-tracking",
  "version": "0.5.4",
  "description": "Tracking library for Gigz",
  "license": "ISC",
  "main": "dist/gigz.js",
  "engines": {
    "node": ">= 0.10.0"
  },
  "maintainers": [
    "Paul-DS"
  ],
  "author": {
    "name": "Paul-DS",
    "url": "https://github.com/Paul-DS"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gigz-fr/gigz-tracking.git"
  },
  "scripts": {
    "watch": "babel src -d dist --experimental -w",
    "lint": "eslint src",
    "build": "npm run build:es && npm run build:umd",
    "build:es": "babel --plugins transform-runtime -d dist src && minify dist/gigz.js -o dist/gigz.min.js",
    "build:umd": "babel --plugins transform-es2015-modules-umd src/gigz.js -o dist/gigz.umd.js && combine-files node_modules/babel-polyfill/dist/polyfill.js,dist/gigz.umd.js dist/gigz.umd.js && minify dist/gigz.umd.js -o dist/gigz.umd.min.js",
    "prepare": "npm run build",
    "postpublish": "git push origin master --follow-tags",
    "deploy": "git pull --rebase origin master && git push origin master"
  },
  "keywords": [],
  "homepage": "https://www.gigz.fr/",
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^20.0.3",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "combine-files": "^1.1.8",
    "eslint": "^3.19.0",
    "eslint-config-prettier": "^2.1.1",
    "eslint-config-xo": "^0.18.2",
    "eslint-plugin-babel": "^4.1.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0"
  }
}
